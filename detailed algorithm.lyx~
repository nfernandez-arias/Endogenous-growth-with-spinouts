\sigma(q) & = & m(q)\xi\\
L(q,m) & = & R_{I}(z_{A}^{I}(q,m))+L_{E}(q,m)\\
g & = & \int_{0}^{\infty}\sigma(q)\eta(q)dq+\int_{0}^{\infty}\int_{0}^{\infty}\tau(q,m)\mu(q,m)dqdqm\\
\eta(q) & = & \text{Stationary distribution of \ensuremath{q} for products in public domain}\\
\mu(q,m) & = & \text{Stationary joint distribution of \ensuremath{q,m} for products not in public domain}
